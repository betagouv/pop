{
  "name": "pop-api",
  "version": "1.3.0-SNAPSHOT",
  "description": "POP API",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon ./src/index.js",
    "dev:env": "nodemon -r dotenv/config ./src/index.js",
    "start": "node ./src/index.js  ",
    "deploy:prod": "npm install && eb deploy pop-api --profil POP",
    "deploy:staging": "npm install && eb deploy pop-api-staging --profil POP",
    "test": "jest -i",
    "sitemap": "nodemon -r dotenv/config src/sitemap/sitemap.js",
    "export:schemas": "node src/export-db-schemas",
    "export:csv": "node src/export-db-csv",
    "export:mapping": "node src/export-db-mapping"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/betagouv/pop.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/betagouv/pop/issues"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./src/__tests__/setup/setup.js"
    ],
    "testPathIgnorePatterns": [
      "./src/__tests__/setup/",
      "./src/__tests__/__notices__/"
    ]
  },
  "prettier": {
    "printWidth": 100
  },
  "homepage": "https://github.com/betagouv/pop",
  "dependencies": {
    "@sentry/node": "^4.6.6",
    "asyncawait": "^1.0.8",
    "aws-sdk": "^2.491.0",
    "aws4": "^1.8.0",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "elasticsearch": "^14.2.2",
    "express": "^4.17.1",
    "express-rate-limit": "^3.5.3",
    "filenamify": "^3.0.0",
    "fs": "^0.0.1-security",
    "generate-password": "^1.4.2",
    "googleapis": "^35.0.0",
    "helmet": "^3.18.0",
    "http-aws-es": "^6.0.0",
    "http-proxy-middleware": "^0.19.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.14",
    "mongoosastic": "^4.5.0",
    "mongoose": "^5.6.4",
    "mongoose-paginate": "^5.0.3",
    "multer": "^1.3.1",
    "nodemailer": "^4.6.7",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "proj4": "^2.5.0",
    "request": "^2.87.0",
    "validator": "^11.1.0",
    "x2js": "3.2.1",
    "xml2js": "^0.4.19",
    "oai-pmh": "^2.0.3",
    "xml": "^1.0.1",
    "moment-timezone": "^0.5.23"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.3.0",
    "commander": "^2.20.0",
    "cross-env": "^5.1.6",
    "csv-parse": "^4.4.3",
    "inquirer": "^6.5.0",
    "jest": "^24.8.0",
    "listr": "^0.14.2",
    "listr-update-renderer": "^0.4.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.19.1",
    "rxjs": "^6.5.2",
    "sitemap": "^2.2.0",
    "supertest": "^3.4.2"
  }
}
