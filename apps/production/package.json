  "scripts": {
    "start": "node src/server/index.js",
    "build:prod": "webpack --env.production --env.NODE_OPTIONS=--max_old_space_size=2048 --env.NODE_ENV=production",
    "build:staging": "webpack --env.staging --env.NODE_OPTIONS=--max_old_space_size=2048 --env.NODE_ENV=staging",
    "dev": "webpack-dev-server --config webpack.dev.config.js --open",
    "deploy:prod": "npm install && npm run build:prod && eb deploy pop-production --profil POP",
    "deploy:staging": "npm install && npm run build:staging && eb deploy pop-production-staging --profil POP",
    "test": "jest"
  },
  "prettier": {
    "printWidth": 100
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/__tests__/setup/setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/src/__tests__/setup/",
      "<rootDir>/src/__tests__/__mocks__/",
      "<rootDir>/src/__tests__/__notices__/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__tests__/__mocks__/fileMock.js",
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "engines": {
    "node": "8.11.x",
    "npm": "5.x"
  },
  "devDependencies": {
